        <field name="thruDate" type="date-time"/>
        <relationship type="one" related="durion.hr.DurHrRole" short-alias="role"/>
        <relationship type="one" related="durion.hr.DurHrPermission" short-alias="permission"/>
    </entity>

</entities>
